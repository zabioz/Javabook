package chapter6;

class Person{
	String name;
	int age;
	
	
	Person() {
		this("no name",1);
	}
	Person(String name, int age){
		this.name = name;
		this.age = age;
		
	}

	Person returnItSelf(){
		return this;
	}
	
}

public class CallAnotherCont {
	public static void main(String [] args) {
		Person noName = new Person();
	}
}
